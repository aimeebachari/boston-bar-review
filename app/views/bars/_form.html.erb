
<%= form_for(@bar) do |f| %>
  <div class="field">
      <%= f.label :name %><br/>
      <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :address %><br/>
    <%= f.text_field :address %>
  </div>

  <div class="field">
    <%= f.label :city %><br/>
    <%= f.text_field :city %>
  </div>

  <div class="field">
    <%= f.label :state %><br/>
    <%= f.text_field :state %>
  </div>

  <div class="field">
    <%= f.label :zip %><br/>
    <%= f.text_field :zip %>
  </div>

  <div class="field">
    <%= f.label :url %><br/>
    <%= f.text_field :url %>
  </div>

  <div class="field">
    <%= f.label :description %><br/>
    <%= f.text_field :description %>
  </div>

  <div class="actions">
    <%= f.submit(value="Submit") %>
  </div>

<% end %>
